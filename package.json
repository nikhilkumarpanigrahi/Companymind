{
  "name": "companymind-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "nodemon server.cjs",
    "start:server": "node server.cjs",
    "clean:dataset": "node scripts/clean-dataset.cjs",
    "load:documents": "node scripts/bulk-load-documents.cjs",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "db:seed": "node seeds/seedDatabase.cjs",
    "db:seed:large": "node seeds/seedDatabase.cjs --file=seeds/large-dataset.json --no-wait",
    "generate:dataset": "node scripts/generate-large-dataset.cjs --count=2000"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.0.0",
    "mongoose": "^8.12.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.20",
    "nodemon": "^3.1.9",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.7.3",
    "vite": "^6.2.0"
  }
